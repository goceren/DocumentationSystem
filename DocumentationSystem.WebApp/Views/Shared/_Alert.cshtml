@model ResultMessage
    <div class="container mt-4">
        <div class="row">
            <div class="col-md-12">
                <div class="alert alert-@Model.Css">
                    <h4 class="alert-title text-center">@Model.Title</h4>
                    <p class="text-center">@Model.Message</p>
                </div>
            </div>
        </div>
    </div>